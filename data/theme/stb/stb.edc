            target: "module.clip";
         }
         program {
            name: "content,show";
            signal: "content,show";
            source:  "enna";
            action:  STATE_SET "show" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "content.clip";
         }
         program {
            name: "content,hide";
            signal: "content,hide";
            source:  "enna";
            action:  STATE_SET "default" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "content.clip";
         }
      }

   }
