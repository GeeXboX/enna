               rel1.to: module.clip;
               rel2.to: module.clip;
               visible: 0;
            }
            description {
               state: "show" 0.0;
               inherit: "default" 0.0;
               color: 255 255 255 255;
               visible: 1;
            }
         }
         /* file browser */
         part {
            name: "browser.clip";
            clip_to: "content.clip";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;

               rel1.relative: 0.0 0.0;
               rel1.offset: 0 16;
               rel1.to: "content.clip";

               rel2.relative: 0.6 1.0;
               rel2.offset: -16 -88;
               rel2.to: "content.clip";
            }
         }
	 part {
	    name: "browser.bg";
	    type: IMAGE;
	    mouse_events: 0;
	    clip_to: "browser.clip";
	    description {
	        state: "default" 0.0;
		rel1.to: "browser.clip";
		//rel1.offset: -5 -5;
		rel2.to: "browser.clip";
		//rel2.offset: 5 5;
		//color: 0 0 0 128 ;
		image {
                     normal: "browser_bg.png";
               	     border: 15 15 15 15;
            	}

	    }
	 }
         part {
            name: "browser.swallow";
            type: SWALLOW;
            mouse_events : 1;
            clip_to: "browser.clip";
            description {
               state: "default" 0.0;
               rel1.to: "browser.clip";
	       rel1.offset: 5 5;
               rel2.to: "browser.clip";
	       rel2.offset: -4 -4;
            }
         }

         /* Mediaplayer */
         part {
            name: "mediaplayer.clip";
            clip_to: "content.clip";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;

               rel1.relative: 0.6 0.0;
               rel1.offset: 16 16;
               rel1.to: "content.clip";

               rel2.relative: 1.0 1.0;
               rel2.offset: -16 -88;
               rel2.to: "content.clip";
            }
         }
         part {
            name: "mediaplayer.swallow";
            type: SWALLOW;
            mouse_events : 1;
            clip_to: "mediaplayer.clip";
            description {
               state: "default" 0.0;

               rel1.to: "mediaplayer.clip";
               rel2.to: "mediaplayer.clip";
            }
         }
      }
      programs {
         program {
            name: "module,show";
            signal: "module,show";
            source:  "enna";
            action:  STATE_SET "show" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "module.clip";

         }
         program {
            name: "module,hide";
            signal: "module,hide";
