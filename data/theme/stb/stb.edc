   group {
      name: "activity/music";
      parts {
         part {
            name: "module.clip";
            type: RECT;
            description {
               state: "default" 0.0;
               color: 255 255 255 255;
               rel1.relative: 1 0;
               rel2.relative: 2 1;
               visible: 0;
            }
            description {
               state: "show" 0.0;
               color : 255 255 255 255;
               visible: 1;
            }
         }

         part {
            name: "content.clip";
            type: RECT;
            clip_to: "module.clip";
            description {
               state: "default" 0.0;
               color: 255 255 255 0;
               rel1.to: module.clip;
               rel2.to: module.clip;
               visible: 0;
            }
            description {
               state: "show" 0.0;
               inherit: "default" 0.0;
               color: 255 255 255 255;
               visible: 1;
            }
         }       
         /* file browser */
         part {
            name: "browser.clip";
            clip_to: "content.clip";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;

               rel1.relative: 0.0 0.0;
               rel1.offset: 64 64;
               rel1.to: "content.clip";

               rel2.relative: 1.0 1.0;
               rel2.offset: -64 -88;
               rel2.to: "content.clip";
            }
         }
         part {
            name: "browser.swallow";
            type: SWALLOW;
            mouse_events : 1;
            clip_to: "browser.clip";
            description {
               state: "default" 0.0;
               rel1.to: "browser.clip";
               rel2.to: "browser.clip";
            }
         }
         /* Mediaplayer */
         part {
            name: "mediaplayer.clip";
            clip_to: "content.clip";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;

               rel1.relative: 0.0 0.0;
               rel1.offset: -8 -8;
               rel1.to: "content.clip";

               rel2.relative: 0.50 1.0;
               rel2.offset: +8 +8;
               rel2.to: "content.clip";
            }
         }
         part {
            name: "mediaplayer.swallow";
            type: SWALLOW;
            mouse_events : 1;
            clip_to: "mediaplayer.clip";
            description {
               state: "default" 0.0;

               rel1.to: "mediaplayer.clip";
               rel2.to: "mediaplayer.clip";
            }
         }
      }
      programs {
         program {
            name: "module,show";
            signal: "module,show";
            source:  "enna";
            action:  STATE_SET "show" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "module.clip";

         }
         program {
            name: "module,hide";
            signal: "module,hide";
            source:  "enna";
            action:  STATE_SET "default" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "module.clip";
 
         }
         program {
            name: "content,show";
            signal: "content,show";
            source:  "enna";
            action:  STATE_SET "show" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "content.clip";
         }
         program {
            name: "content,hide";
            signal: "content,hide";
            source:  "enna";
            action:  STATE_SET "default" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "content.clip";
         }
         program {
            name: "mediaplayer,show";
            signal: "mediaplayer,show";
            source:  "enna";
            action:  STATE_SET "show" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "mediaplayer.clip";
         }
         program {
            name: "mediaplayer,hide";
            signal: "mediaplayer,hide";
            source:  "enna";
            action:  STATE_SET "default" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "mediaplayer.clip";
         }
     
      }
   }
   group
   {
      name: "enna/browser";
      parts
      {

         part {
            name: "clip";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;
               rel1.relative : 0 0;
               rel2.relative : 1 1;
            }
         }

         part {
            name: "enna.swallow.header";
            type: SWALLOW;
            clip_to: "clip";
            description {
               min: 0 64;
               max: 10000 64;
            
               state: "default" 0.0;
               align: 0 0;
               rel1.offset: 10 0;
               rel1.relative: 0 0;

               rel2.relative: 1 1;
               rel2.offset: -10 0;
            }
         }
        
         part {
            name: "enna.swallow.content";
            type: SWALLOW ;
            clip_to: "clip";
            description {
               state: "default" 0.0;
               rel1.to: "enna.swallow.header";
               rel1.relative: 0 1;
               rel1.offset: 0 2;
               rel2.relative: 1 1;
            }
         }
         part {
            name : "enna.clip.letter";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               color: 0 0 0 0;
               visible: 1;
               aspect_preference: BOTH;
               aspect: 1 1;
               rel1 {relative: 0.5 0.5;}
               rel2 {relative: 0.5 0.5;};
            }
            description {
               state: "show" 0.0;
               visible: 1;
               aspect_preference: BOTH;
               aspect: 1 1;
               color: 255 255 255 255;
               rel1 {relative: 0.35 0.35;}
               rel2 {relative: 0.65  0.65;};
            }
         }
         part { name: "enna.swallow.letter";
            mouse_events: 0;
            clip_to: "enna.clip.letter";
            type: SWALLOW;
            description { state: "default" 0.0;
               visible: 1;
               fixed: 1 1;
               color: 255 255 255 50;
               rel1.relative: 0 0;
               rel1.to: "enna.clip.letter";
               rel2.relative: 1 1;
               rel2.to: "enna.clip.letter";
            }

         }
      }
      programs
      {
         program {
            name: "letter,show";
            signal: "letter,show";
            source:  "enna";
            action:  STATE_SET "show" 0.0;
            transition: "ACCELERATE" 0.3;
            target: "enna.clip.letter";
         }
         program {
            name: "letter,hide";
            signal: "letter,hide";
            source:  "enna";
            action:  STATE_SET "default" 0.0;
            transition: "ACCELERATE" 0.3;
            target: "enna.clip.letter";
         }

      }
   }

   group
   {
      name: "enna/browser/header";
