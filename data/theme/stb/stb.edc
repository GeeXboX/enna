group {
   name: "activity/configuration";
   parts {
      part {
         name: "module.clip";
         type: RECT;
         description {
            state: "default" 0.0;
            color: 255 255 255 0;
            visible: 0;
         }
         description {
            state: "show" 0.0;
            color: 255 255 255 255;
            visible: 1;
         }
      }


      /* main config menu */
      part {
         name: "menu.clip";
         type: RECT;
         clip_to:  "module.clip";
         description {
            state: "default" 0.0;
            color: 255 255 255 0;
            visible: 0;
            rel1.relative: 0.0 0.0;
            rel2.relative: 0.4 1.0;
         }
         description {
            state: "show" 0.0;
            color: 255 255 255 255;
            visible: 1;
            rel1.relative: 0.0 0.0;
            rel2.relative: 0.4 1.0;
         }
      }

      part {
