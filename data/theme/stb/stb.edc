fonts{
   font:"Comfortaa_Bold.ttf" "Comfortaa_Bold";
   font:"Comfortaa_Regular.ttf" "Comfortaa_Regular";
   font:"Comfortaa_Thin.ttf" "Comfortaa_Thin";
}

collections
{
#include "icons.edc"
#include "elm/elm.edc"
   group { name: "enna/main/layout";
      images {
         image: "bg.png" COMP;
         image: "bg_tile.png" COMP;
         image: "menu_bg.png" COMP;
      }
      parts {
         /* background  */
         part {
            type: IMAGE;
            description {
               state: "default" 0.0;
               image {
                  normal: "bg.png";
               }
            }
         }
         part {
            type: IMAGE;
            description {
               state: "default" 0.0;
               image {
                  normal: "bg_tile.png";
               }
               fill {
                  size {
                     relative: 0.0 0.0;
                     offset: 20 20;
                  }  
               }
            }
         }
         /* MainMenu */
         part { name: "mainmenu.clip";
            type: RECT;
            description { state: "default" 0.0;
               color: 255 255 255 255;
               visible: 1;
               rel1.relative:  0 1;
               rel1.offset: 0 -86;
               rel2.relative:  1 1;
               rel2.offset: 0 202;
            }
            description { state: "visible" 0.0;
               color: 255 255 255 255;
               visible: 1;
               rel1.relative:  0 1;
               rel1.offset: 0 -288;
               rel2.relative:  1 1;
            }
         }
         part {
            type: IMAGE;
            clip_to: "mainmenu.clip";
            description {
               state: "default" 0.0;
               rel1.to: "mainmenu.clip";
               rel2.to: "mainmenu.clip";
               image {
                  normal: "menu_bg.png";
               }
               fill {
                  size {
                     relative: 0.0 1.0;
                     offset: 20 0;
                  }  
               }

            }
         }
         part {
            name: "enna.mainmenu.swallow";
            clip_to: "mainmenu.clip";
            type: SWALLOW;
            description {
               rel1.relative: 0 1;
               rel1.offset: 0 -176;
               rel1.to: "mainmenu.clip";
               rel2.offset: 0 -22;
               rel2.to: "mainmenu.clip";
            }
         }
         /* content */
         part { name: "enna.content.swallow";
            type: SWALLOW;
            description { state: "default" 0.0;
            }
         }
        
      }
      programs {
         program { name: "mainmenu,show";
            signal: "mainmenu,show";
            source: "enna";
            action: STATE_SET "visible" 0.0;
            transition: ACCELERATE 0.5;
            target: "mainmenu.clip";
         }
         program { name: "mainmenu,hide";
            signal: "mainmenu,hide";
            source: "enna";
            action: STATE_SET "default" 0.0;
            transition: DECELERATE 0.5;
            target: "mainmenu.clip";
         }

      }
   }

   /* EXIT */
   group {
      name: "enna/exit/layout";
      parts {
         part {
            name: "enna.label.swallow";
            type: SWALLOW;
            mouse_events:  0;
            scale: 1;
            description { state: "default" 0.0;
               visible: 1;
               align: 0.5 0.5;
               rel1.relative: 0.0 0.0;
               rel1.offset: 0 32;
               rel2.relative: 1.0 0.35;
            }
         }
         part {
            name: "enna.content.swallow";
            mouse_events: 1;
            type: SWALLOW;
            description{
               state: "default" 0.0;
               rel1.relative: 0.05 0.35;
               rel2.relative: 0.95 0.90;
            }
         }
      }
   }

   /* BOX */

   group {
      name: "enna/box/layout/list";
      alias: "enna/box/layout/exit";
      data {
         item: "scroll_horizontal" "off";
         item: "bounce_horizontal" "off";
         item: "scroll_vertical" "off";
         item: "bounce_vertical" "off";
         item: "orientation" "horizontal";
      }
      parts {
         part {
            name: "enna.swallow.content";
            type: SWALLOW;
            description {
               state: "default" 0.0;
            }
         }
      }     
   }
   group {
      name: "enna/box/item/list";
      images {
         image: "menu_selector.png" COMP;
      }
      parts {
         part { name: "bg";
            mouse_events: 0;
            description { state: "default" 0.0;
               visible: 0;
               color: 255 255 255 0;
               min: 154 154;
               max: 154 154;
               image {
                  normal: "menu_selector.png";
                  border: 30 30 30 30;
               }
            }
            description { state: "selected" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               color: 255 255 255 255;
            }
         }
         part { name: "enna.swallow.icon";
            type: SWALLOW;
            description { state: "default" 0.0;
               //fixed: 1 0;
               align: 0.5 0  ;
               aspect: 1.0 1.0;
               min: 128 128;
               max: 128 128;
               aspect_preference: BOTH;
            }
         }
         part { name: "enna.text.label";
            type:           TEXT;
            effect:         SHADOW;
            mouse_events:   0;
            //ale: 1;
            description {
               state: "default" 0.0;
               fixed: 0 0;
               rel1 {
                  to_y: "enna.swallow.icon";
                  to_x: "bg";
                  relative: 0.0  1.0;
               }
               rel2 {
                  to:     "bg";
                  relative: 1.0  1.0;
                  offset: -1 -20;
               }
               color: 255 255 255 255;
               color3: 147 147 147 255;
               text {
                  font: "Comfortaa";
                  size: 20;
                  align: 0.5 0.5;
               }
            }
         }
         part {
            name:           "event";
            type:           RECT;
            repeat_events: 1;
            description {
               state: "default" 0.0;
               color: 0 0 0 0;
            }
         }
      } 
      programs {
         program {
            name:    "go_active";
            signal:  "select";
            source:  "enna";
            action:  STATE_SET "selected" 0.0;
            target:  "bg";
            transition: ACCELERATE 0.2;
         }
         program {
            name:    "go_passive";
            signal:  "unselect";
            source:  "enna";
            action:  STATE_SET "default" 0.0;
            target:  "bg";
            transition: LINEAR 0.2;
         }
      }
   }

