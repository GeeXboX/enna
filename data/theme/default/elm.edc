                    image {
                        normal: "bt_base2.png";
                        border: 7 7 7 7;
                    }
                    image.middle: SOLID;
                }
                description { state: "clicked" 0.0;
                    inherit: "default" 0.0;
                    image.normal: "bt_base1.png";
                    image.middle: SOLID;
                }
                description { state: "disabled" 0.0;
                    inherit:  "default" 0.0;
