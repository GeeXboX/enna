                    //~ image {
                        //~ normal: "bt_dis_base.png";
                        //~ border: 4 4 4 4;
                    //~ }
                }
            }
            part { name: "elm.swallow.content";
                type: SWALLOW;
                description { state: "default" 0.0;
                    fixed: 1 0;
                    visible: 0;
                    align: 0.0 0.5;
                    rel1.offset: 4 4;
                    rel2.offset: 3 -5;
                    rel2.relative: 0.0 1.0;
                }
                description { state: "visible" 0.0;
                    inherit: "default" 0.0;
                    fixed: 1 0;
                    visible: 1;
                    aspect: 1.0 1.0;
                    aspect_preference: VERTICAL;
                    rel2.offset: 4 -5;
                }
                description { state: "icononly" 0.0;
                    inherit: "default" 0.0;
                    fixed: 0 0;
                    visible: 1;
