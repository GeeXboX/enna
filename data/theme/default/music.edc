 group {
    name: "module/music";
    parts {
       part {
          name: "enna.clip.module";
          type: RECT;
          description {
             state: "default" 0.0;
             color: 0 0 0 0;
             visible: 0;
          }
          description {
             state: "show" 0.0;
             color : 255 255 255 255;
             visible: 1;
          }
       }
       /*part {
          name: "bg";
          clip_to: "enna.clip.module";
          type: RECT;
          description {
             state: "default" 0.0;
             color: 0 255 0 15;
          }
          }*/


       part {
          name: "enna.swallow.list";
          type: SWALLOW;
          mouse_events : 1;
          clip_to: "enna.clip.module";
          description {
             state: "default" 0.0;
             color: 255 255 255 255;
             rel1 {relative: 0.1 -0.9;to: "enna.clip.module";}
             rel2 {relative: 0.9 -0.1;to: "enna.clip.module";}
          }
          description {
             state: "show" 0.0;
             color: 255 255 255 255;
             rel1 {relative: 0.1 0.1;to: "enna.clip.module";}
             rel2 {relative: 0.9 0.9;to: "enna.clip.module";}
          }
       }
       part {
          name: "enna.swallow.location";
          type: SWALLOW;
          mouse_events : 1;
          clip_to: "enna.clip.module";
          description {
             state: "default" 0.0;
             color: 255 255 255 255;
             rel1 {relative: 0.0 1.0;}
             rel2 {relative: 0.8 1.1;}
          }
          description {
             state: "show" 0.0;
             color: 255 255 255 255;
             rel1 {relative: 0.0 0.9;}
             rel2 {relative: 0.8 1.0;}
          }
       }
    }
    programs {
       program {
          name: "module,show";
          signal: "show";
          source:  "enna";
          action:  STATE_SET "show" 0.0;
          transition: SINUSOIDAL 0.5;
          target: "enna.clip.module";
          target: "enna.swallow.list";
          target: "enna.swallow.location";
       }
       program {
          name: "module,hide";
          signal: "hide";
          source:  "enna";
          action:  STATE_SET "default" 0.0;
          transition: SINUSOIDAL 0.5;
          target: "enna.clip.module";
          target: "enna.swallow.list";
          target: "enna.swallow.location";
       }
    }
 }
