         state: "default" 0.0;
	 rel1.relative: 0.45 0;
	 rel2.relative: 1.0 1.0;
         image.normal: "text_border.png";
	 image.border: 10 10 10 10;
         }
      }
        part {
	 name:           "enna.text.title";
         clip_to: "enna.clip.player";
	 type:           TEXT;
	 mouse_events:   0;
	 description {
	    state: "default" 0.0;
	    rel1 {
               relative: 0  0.05;
	       to: "text_border";
	    }
	    rel2 {
	       relative: 1.0  0.15;
	       to: "text_border";
	    }
            color: 200 200 200 200;
            color3: 128 128 128 200;
            align: 0.5 0.5;
	    text {
               text:     "Title";
               font:     "content_bd";
               size:     25;
               fit: 1 1;
               min: 1 0;
               //max: 1 1;;
            }
	 }
      }

