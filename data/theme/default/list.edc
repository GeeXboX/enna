images {
   image: "menu_sel_bg.png" COMP;
   image: "menu_sel_fg.png" COMP;
   image: "letter_bg.png" COMP;
}

group {
   name: "enna/list/item";

   parts {

      part { name: "bg";
         mouse_events: 0;
         description { state: "default" 0.0;
            visible: 0;
            color: 255 255 255 0;
            rel1 {
               relative: 0.0 0.0;
               offset: -5 -3;
            }
            rel2 {
               relative: 1.0 1.0;
               offset: 4 5;
            }
            image {
               normal: "menu_sel_bg.png";
               border: 8 8 5 9;
            }
         }
         description { state: "selected" 0.0;
            inherit: "default" 0.0;
            visible: 1;
            color: 255 255 255 128;
            rel1 {
               relative: 0.0 0.0;
               offset: -2 0;
            }
            rel2 {
               relative: 1.0 1.0;
               offset: 1 2;
            }
         }
      }


      part {
	 name:          "enna.swallow.icon";
	 type:          SWALLOW;
         mouse_events: 0;
	 description {
	    state:    "default" 0.0;
	    align:    0.0 0.5;
	    aspect:   1.0 1.0;
	    aspect_preference: VERTICAL;
            //	    fixed:    1 1;
	    rel1 {
	       relative: 0.0  0.0;
	       offset:   4    5;
	    }
	    rel2 {
	       relative: 0.0  1.0;
	       offset:   4   -19;
	    }
	 }
      }
      part {
	 name:           "enna.text.label";
	 type:           TEXT;
	 mouse_events:   0;
	 description {
	    state: "default" 0.0;
	    rel1 {
	       to_x:     "enna.swallow.icon";
	       relative: 1.2  0.0;
	       offset:   4 4;
	    }
	    rel2 {
	       relative: 1.2  1.0;
	       offset:   -5  -19;
	    }
            color: 200 200 200 200;
            color3: 128 128 128 200;
            align: 0 0.5;
	    text {
               text:     "Item Label";
               font:     "content";
               size:     25;
               //min: 1 1;
               max: 1 1;;
            }
	 }
      }
      part { name: "fg";
         mouse_events: 0;
         description { state: "default" 0.0;
            visible: 0;
            color: 255 255 255 0;
            rel1.to: "bg";
            rel2.to: "bg";
            image {
               normal: "menu_sel_fg.png";
               border: 15 31 15 31;
            }
         }
         description { state: "selected" 0.0;
            inherit: "default" 0.0;
            visible: 1;
            color: 255 255 255 50;
         }
      }
      part {
	 name:           "event";
	 type:           RECT;
         mouse_events: 1;

	 description {
	    state: "default" 0.0;
	    color: 0 0 0 0;
	 }
      }
   }
   programs {

      program { name: "sel";
         signal: "enna,state,selected";
         source: "enna";
         action: STATE_SET "selected" 0.0;
         transition: LINEAR 0.2;
         target: "bg";
         target: "fg";
      }
      program { name: "unsel";
         signal: "enna,state,unselected";
         source: "enna";
         action: STATE_SET "default" 0.0;
         transition: LINEAR 0.1;
         target: "bg";
         target: "fg";
      }
   }
}

