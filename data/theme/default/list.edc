group {
   name: "enna/list";

   parts {

      part {
         name: "enna.swallow.content";
         type: "SWALLOW";
         clip_to: "enna.clip.list";
         description {
            state: "default" 0.0;
            color: 255 255 255 255;
            rel1.to: "enna.clip.list";
            rel2.to: "enna.clip.list";
         }
      }
      part {
         name: "enna.clip.list";
         type: "RECT";
         mouse_events: 0;
         description {
            state: "default" 0.0;
            color: 255 255 255 255;
            rel1 {relative: 0 0;}
            rel2 {relative: 1 1;};
         }
         description {
            state: "left" 0.0;
            color: 255 255 255 0;
            rel1 {relative: -2 0;}
            rel2 {relative: -1  1;};
         }
         description {
            state: "right" 0.0;
            color: 255 255 255 0;
            rel1{relative: 1 0;}
            rel2 {relative: 2 1;};
         }
      }
      part {
         name : "enna.clip.letter";
         type: RECT;
         mouse_events: 0;
          description {
             state: "default" 0.0;
             color: 0 0 0 0;
             visible: 1;
             aspect_preference: BOTH;
             aspect: 1 1;
             rel1 {relative: 0.5 0.5;}
             rel2 {relative: 0.5 0.5;};
         }
         description {
            state: "show" 0.0;
            visible: 1;
            aspect_preference: BOTH;
            aspect: 1 1;
            color: 255 255 255 255;
            rel1 {relative: 0.35 0.35;}
            rel2 {relative: 0.65  0.65;};
         }
      }
      part { name: "letter_bg";
         mouse_events: 0;
         description { state: "default" 0.0;
            visible: 1;
            color: 255 255 255 50;
            rel1.relative: 0 0;
            rel1.to: "enna.clip.letter";
            rel2.relative: 1 1;
            rel2.to: "enna.clip.letter";
            image {
               normal: "letter_bg.png";
               border: 22 22 22 22;
            }
         }

      }
      part {
	 name:           "enna.text.letter";
	 type:           TEXT;
         clip_to: "enna.clip.letter";
	 mouse_events:   0;
	 description {
	    state: "default" 0.0;
	    rel1 {
               relative: 0 0;
               to: "enna.clip.letter";
               offset: 22 22;
	    }
	    rel2 {
	       relative: 1 1;
	       to: "enna.clip.letter";
               offset: -23 -23;
	    }
            color: 200 200 200 200;
            color3: 128 128 128 200;
            align: 0.5 0.5;
	    text {
               text:     "Item Label";
               font:     "content";
               size:     50;
               fit: 0 1;
               min: 0 0;
               max: 0 0;
            }
	 }
      }

   }
   programs{
      program {
         name: "list,left,now";
         signal: "list,left,now";
         source:  "enna";
         action:  STATE_SET "left" 0.0;
         target: "enna.clip.list";
      }
      program {
         name: "list,right,now";
         signal: "list,right,now";
         source:  "enna";
         action:  STATE_SET "right" 0.0;
         target: "enna.clip.list";
      }
      program {
         name: "list,right";
         signal: "list,right";
         source:  "enna";
         action:  STATE_SET "right" 0.0;
         transition: "ACCELERATE" 0.3;
         target: "enna.clip.list";
         after: "transition,end";
      }
      program {
         name: "list,left";
         signal: "list,left";
         source:  "enna";
         action:  STATE_SET "left" 0.0;
         transition: "ACCELERATE" 0.3;
         target: "enna.clip.list";
         after: "transition,end";
      }
      program {
         name: "list,default";
         signal: "list,default";
         source:  "enna";
         action:  STATE_SET "default" 0.0;
         transition: "ACCELERATE" 0.3;
         target: "enna.clip.list";
      }
      program {
         name: "transition,end";
         action: SIGNAL_EMIT "list,transition,end" "edje";
      }

     program {
         name: "letter,show";
         signal: "letter,show";
         source:  "enna";
         action:  STATE_SET "show" 0.0;
         transition: "ACCELERATE" 0.3;
         target: "enna.clip.letter";
      }
      program {
         name: "letter,hide";
         signal: "letter,hide";
         source:  "enna";
         action:  STATE_SET "default" 0.0;
         transition: "ACCELERATE" 0.3;
         target: "enna.clip.letter";
      }
   }
}

