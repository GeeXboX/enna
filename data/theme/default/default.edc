            description{
               state: "default" 0.0;
               color: 255 255 255 255;
               rel1{relative: 0 0;to: "enna.clip.mainmenu";};
               rel2{relative: 1 1;to: "enna.clip.mainmenu";};
            }
         }
         part {
            name: "enna.clip.module";
            type: RECT;
            mouse_events : 0;
            description{
               state: "default" 0.0;
               visible: 0;
               color: 0 0 0 0;
               rel1{relative: 0 0;};
               rel2{relative: 1 1;};
            }
            description{
               state: "show" 0.0;
               visible: 1;
               color: 255 255 255 255;
               rel1{relative: 0 0;};
               rel2{relative: 1 1;};
            }
         }
         part {
            name: "enna.clip.mainmenu";
            type: RECT;
            mouse_events : 0;
            description{
               state: "default" 0.0;
               visible: 0;
               color: 0 0 0 255;
               rel1{relative: 0 0.0;};
               rel2{relative: 1 1;};
            }
            description{
               state: "show" 0.0;
               visible: 1;
               color: 255 255 255 255;
               rel1{relative: 0 0;};
               rel2{relative: 1 1;};
            }
         }
