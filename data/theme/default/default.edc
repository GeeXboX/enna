   group {
      name: "enna";
      min: 800 480;
      parts {

         part {
            name: "enna.swallow.background";
            type: SWALLOW;
            mouse_events : 0;
            description{
               state: "default" 0.0;
               color:  255 255 255 255;
               rel1{relative: 0 0;};
               rel2{relative: 1 1;};
            }
         }
         part {
            name: "enna.swallow.module";
            type: SWALLOW;
            clip_to: "enna.clip.module";
            mouse_events : 1;
            description{
               state: "default" 0.0;
               color:  255 255 255 255;
               rel1{relative: 0 0;};
               rel2{relative: 1 1;};
