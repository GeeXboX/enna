      programs {
         program {
            name: "module,show";
            signal: "module,show";
            source:  "enna";
            action:  STATE_SET "show" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "enna.clip.module";
         }
         program {
            name: "module,hide";
            signal: "module,hide";
            source:  "enna";
            action:  STATE_SET "default" 0.0;
            transition: SINUSOIDAL 0.5;
            target: "enna.clip.module";
         }
         program {
            name: "mainmenu,show";
            signal: "mainmenu,show";
            source:  "enna";
            action:  STATE_SET "show" 0.0;
            transition: SINUSOIDAL 1.2;
            target: "enna.clip.mainmenu";
         }
         program {
            name: "mainmenu,hide";
            signal: "mainmenu,hide";
            source:  "enna";
